---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>LI Dabing's blog</title>
    <link>http://blog.lidaobing.com/</link>
    <atom:link href="http://blog.lidaobing.com/rss.xml" rel="self" type="application/rss+xml" />
    <description>LI Daobing's blog</description>
    <language>zh-cn</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://blog.lidaobing.com{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>lidaobing@gmail.com (LI Daobing)</author>
      <guid>http://blog.lidaobing.com{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}
  </channel>
</rss>
